FoamFile
{
    version     2.0;
    format      ascii;
    class       uniformDimensionedVectorField;
    location    "constant";
    object      g;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Although it may not be used, it's still need to be defined to run the simulation

dimensions      [0 1 -2 0 0 0 0];
value ( 0 0 -9.81e-6 ); 


