FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "300";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    repeat_boundary
    {
        type            zeroGradient;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 1;
    }
}


