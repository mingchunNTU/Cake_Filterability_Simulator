FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "8000";
    object      Ksl;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    repeat_boundary
    {
        type            zeroGradient;
    }
}


